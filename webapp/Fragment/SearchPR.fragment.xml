<core:FragmentDefinition
	xmlns="sap.m" xmlns:fb="sap.ui.comp.filterbar"
	xmlns:l="sap.ui.layout" 
	xmlns:core="sap.ui.core">
	<Dialog title="Created By Search" >
        <content>
            <fb:FilterBar reset="onReset" search="onSearch" showRestoreButton="true" showClearButton="true">
			<fb:filterGroupItems>
				<fb:FilterGroupItem groupName="G1" name="A" label="Created BY" partOfCurrentVariant="true" visibleInFilterBar="true">
					<fb:control>
                        <Input type="Text"/>
					</fb:control>
				</fb:FilterGroupItem>

				<fb:FilterGroupItem groupName="G1" name="B" label="FIRST Name"  visibleInFilterBar="true">
					<fb:control>
						<Input type="Text"/>
					</fb:control>
				</fb:FilterGroupItem>

				<fb:FilterGroupItem groupName="G1"  name="A1" label="Last Name" visibleInFilterBar="true">
					<fb:control>
						<Input type="Text"/>
					</fb:control>
				</fb:FilterGroupItem>
                <fb:FilterGroupItem groupName="G1"  name="A2" label="Fullname" visibleInFilterBar="true">
					<fb:control>
						<Input type="Text"/>
					</fb:control>
				</fb:FilterGroupItem>
			</fb:filterGroupItems>

	    </fb:FilterBar>
        <Table sticky="ColumnHeaders,HeaderToolbar,InfoToolbar" >
            <columns>
                <Column >
                    <Label text="Created By"/>
                </Column>
               
            </columns>
            <ColumnListItem type="Navigation" press="onLineItemPress">
                <cells >
                    <Text text="ABC" />
                   
                </cells>
            </ColumnListItem>
        </Table>
        </content>
		
	</Dialog>
</core:FragmentDefinition>